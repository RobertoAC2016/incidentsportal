@model response_sql

<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
    <div class="container-fluid">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Carlitos</a>
        <div class="navbar-brand">
            <ul class="navbar-nav flex-grow-1">
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="Register">
                        <i class="bi bi-person-lines-fill"></i>
                        Register
                    </a>
                </li>
                <li>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="Login">
                        <i class="bi bi-person"></i>
                        Login
                    </a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<div class="row">
    <div class="col-md-3">&nbsp;</div>
    <div class="col-md-6">

        <div class="card">
            <div class="card-header">
                Register for <b>@Model.valor</b>
            </div>
            <div class="card-body">

                <div class="row">
                    <div class="col-12 text-center">
                        Register status @Model.message
                    </div>
                </div>

                <div class="row">&nbsp;</div>

                <div class="row">
                    <a href="@Url.Action("Index", "Home")" class="btn btn-secondary">
                        <i class="bi bi-caret-left"></i>
                        Back
                    </a>
                </div>

            </div>
        </div>

    </div>
    <div class="col-md-3">&nbsp;</div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }

